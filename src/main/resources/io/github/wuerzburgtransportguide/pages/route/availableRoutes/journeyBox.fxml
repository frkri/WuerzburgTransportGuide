<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="5.0" styleClass="backgroundBoxes" GridPane.rowIndex="0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
	</padding>
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2">
			<children>
				<Label id="date" styleClass="infoBoxes" />
				<Label id="time" styleClass="infoBoxes" />
			</children>
		</HBox>
		<HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.columnSpan="2">
			<children>
				<Label styleClass="infoBoxes" text="Interchange:" />
				<Label id="interchange" styleClass="infoBoxes" />
			</children>
		</HBox>
		<HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="4" GridPane.columnSpan="2">
			<children>
				<Label styleClass="infoBoxes" text="Duration:" />
				<Label id="duration" styleClass="infoBoxes" />
			</children>
		</HBox>
		<HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="6" GridPane.columnSpan="2">
			<children>
				<Label styleClass="infoBoxes" text="Arrival:" textAlignment="RIGHT" />
				<Label id="arrival" styleClass="infoBoxes" />
			</children>
		</HBox>
	</children>
	<stylesheets>
		<URL value="@journeyBox.css" />
	</stylesheets>
</GridPane>
